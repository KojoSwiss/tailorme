<%= simple_form_for @tailor do |f| %>
  <%= f.input :shop_name %>
  <%= f.input :description %>
  <%= f.input :review %>
  <%= f.input :address %>
  <%= f.input :rating, collection: 1..5 %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit, class:"btn btn-secondary" %>
<% end %>
